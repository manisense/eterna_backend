# Server Configuration
PORT=3000

# PostgreSQL Database
DATABASE_URL=upstash_url

# Redis (BullMQ Queue)
REDIS_URL=redis://localhost:6379

# Solana Configuration (for DEX execution)
# Use devnet for testing: https://api.devnet.solana.com
# For mainnet: https://api.mainnet-beta.solana.com
SOLANA_RPC_URL=https://api.devnet.solana.com

# Wallet Private Key (base58 encoded)
# IMPORTANT: Never commit real private keys to version control!
# Generate a new keypair for devnet testing:
#   solana-keygen new --outfile ~/.config/solana/devnet.json
#   solana config set --keypair ~/.config/solana/devnet.json
#   solana config set --url devnet
#   solana airdrop 2
# 
# Export as base58:
#   cat ~/.config/solana/devnet.json | node -e "console.log(require('bs58').encode(Buffer.from(JSON.parse(require('fs').readFileSync('/dev/stdin','utf8')))))"
WALLET_PRIVATE_KEY=your_base58_private_key_here

# Optional: Custom RPC with higher rate limits
# SOLANA_RPC_URL=https://your-custom-rpc.com
